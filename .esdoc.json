{
  "source": ".\\src\\app\\js",
  "destination": ".\\docs",
  "includes": ["\\.jsx$", "\\.js$"],
  "excludes": ["\\.config\\.js$"],
  "plugins": [
	{"name": "esdoc-standard-plugin"},
	{"name": "esdoc-jsx-plugin", "option": {"enable": true}},
	{
      "name": "esdoc-ecmascript-proposal-plugin",
      "option": {
        "classProperties": true,
        "objectRestSpread": true,
        "doExpressions": true,
        "functionBind": true,
        "functionSent": true,
        "asyncGenerators": true,
        "decorators": true,
        "exportExtensions": true,
        "dynamicImport": true
      }
    }
  ]
}